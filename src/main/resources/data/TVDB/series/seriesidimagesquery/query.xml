<?xml version="1.0" encoding="UTF-8"?>
<Query>
	<Description>Query images for the given series ID.</Description>
	<Url><![CDATA[https://api.thetvdb.com/series/{id}/images/query]]></Url>
	<Method type="GET"></Method>
	<Parameters>
		<Headers>
			<Parameter name="Accept-Language" type="JSON"></Parameter>
		</Headers>
		<Path>
			<Parameter name="id" type="JSON"></Parameter>
		</Path>
	</Parameters>
	<Response>response.json</Response>
</Query>
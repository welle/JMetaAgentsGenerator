<?xml version="1.0" encoding="UTF-8"?>
<Query>
	<Description>Returns actors for the given series id</Description>
	<Url><![CDATA[https://api.thetvdb.com/series/{id}/actors]]></Url>
	<Method type="GET"></Method>
	<Parameters>
		<Path>
			<Parameter name="id" type="JSON"></Parameter>
		</Path>
	</Parameters>
	<Response>response.json</Response>
</Query>
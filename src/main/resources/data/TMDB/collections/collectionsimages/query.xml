<?xml version="1.0" encoding="UTF-8"?>
<Query>
	<Description>Get the images for a collection by id.</Description>
	<Url><![CDATA[https://api.themoviedb.org/3/collection/{collection_id}/images?api_key={api_key}&language={language}]]></Url>
	<Method type="GET">question.json</Method>
	<Parameters>
		<Path>
			<Parameter name="collection_id" type="PARAM">collection_id</Parameter>
			<Parameter name="language" type="PARAM">language</Parameter>
		</Path>
	</Parameters>
	<Response>response.json</Response>
</Query>
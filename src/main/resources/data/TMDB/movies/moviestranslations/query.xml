<?xml version="1.0" encoding="UTF-8"?>
<Query>
	<Description>Get a list of translations that have been created for a movie.</Description>
	<Url><![CDATA[https://api.themoviedb.org/3/movie/{movie_id}/translations?api_key={api_key}]]></Url>
	<Method type="GET">question.json</Method>
	<Parameters>
		<Path>
			<Parameter name="movie_id" type="PARAM">movie_id</Parameter>
		</Path>
	</Parameters>
	<Response>response.json</Response>
</Query>
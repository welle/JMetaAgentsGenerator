<?xml version="1.0" encoding="UTF-8"?>
<Query>
	<Description>Get a companies logos by id.<br/>There are two image formats that are supported for companies, PNG's and SVG's.</Description>
	<Url><![CDATA[https://api.themoviedb.org/3/company/{company_id}?api_key={api_key}]]></Url>
	<Method type="GET">question.json</Method>
	<Parameters>
		<Path>
			<Parameter name="company_id" type="PARAM">company_id</Parameter>
		</Path>
	</Parameters>
	<Response>response.json</Response>
</Query>
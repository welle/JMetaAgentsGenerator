<?xml version="1.0" encoding="UTF-8"?>
<Query>
	<Description>Get the list of official genres for TV shows.</Description>
	<Url><![CDATA[https://api.themoviedb.org/3/genre/tv/list?api_key={api_key}&language={language}]]></Url>
	<Method type="GET">question.json</Method>
	<Parameters>
		<Path>
			<Parameter name="language" type="PARAM">language</Parameter>
		</Path>
	</Parameters>
	<Response>response.json</Response>
</Query>